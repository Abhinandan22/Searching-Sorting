import java.io.*;
import java.util.*;
public class MyClass {
    public static void main(String args[]) {
      
      Scanner sc = new Scanner(System.in);
      
      int num = sc.nextInt();
      int arr[] = new int[num];
      
      for(int i = 0 ; i < num ; i++){
          arr[i] = sc.nextInt();
      }
     
      int numtofind = sc.nextInt();
      
      for(int i = 0 ; i < num ; i++){
          
          if(arr[i]==numtofind)
          System.out.println(i);
      }
      
    }
}
